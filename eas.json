{
  "build": {
    "adhoc": {
      "ios": {
        "distribution": "internal",
        "simulator": false,
        "autoIncrement": true,
        "buildConfiguration": "Release"
      },
      "env": {
        "NODE_ENV": "production"
      }
    },
    "debug": {
      "ios": {
        "simulator": false,
        "developmentClient": true,
        "distribution": "internal"
      }
    },
    "preview": {
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false,
        "autoIncrement": true,
        "distribution": "store"
      }
    },
    "production": {
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "simulator": false,
        "autoIncrement": true,
        "distribution": "store"
      },
      "autoIncrement": true
    }
  },
  "cli": {
    "appVersionSource": "remote"
  }
}